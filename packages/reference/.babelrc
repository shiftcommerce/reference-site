{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": ["inline-dotenv"]
    },
    "test": {
      "presets": [
        ["@babel/env", { "modules": "commonjs" }],
        ["next/babel"]
      ],
      "plugins": ["transform-inline-environment-variables"]
    }
  },
  "plugins": [
    ["transform-define"]
  ]
}
