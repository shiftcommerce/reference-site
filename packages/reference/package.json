{
  "name": "shift-front-end-react",
  "version": "0.0.1",
  "main": "server/server.js",
  "repository": "https://github.com/shiftcommerce/shift-front-end-react.git",
  "author": "Shift Commerce",
  "license": "MIT",
  "engines": {
    "node": "^11.13.0"
  },
  "scripts": {
    "devserver": "npm rebuild node-sass && NODE_ENV=development node server/server.js",
    "build": "next build client",
    "build-and-test-develop": "start-server-and-test devserver http://localhost:3000 cy:run",
    "build-and-test-production": "build && start-server-and-test start http://localhost:3000 cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:unit": "jest --config=jest-test.config.js",
    "test:integration": "jest --config=jest-puppeteer.config.js",
    "test:integration:head": "JEST_PUPPETEER_CONFIG=jest-puppeteer-head.config.js jest --config=jest-puppeteer-head.config.js",
    "test:integration:server": "PORT=3001 API_HOST_PROXY=http://localhost:3001 node server/server.js",
    "jest-watch": "jest --watch",
    "jest-coverage": "jest --coverage",
    "jest-update-snapshot": "jest --updateSnapshot",
    "heroku-postbuild": "next build client",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint client/scss",
    "lint:css:fix": "stylelint client/scss --fix",
    "start": "NODE_ENV=production node server/server.js"
  },
  "dependencies": {
    "@shiftcommerce/shift-next": "^0.7.2",
    "@shiftcommerce/shift-react-components": "^0.9.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.16.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-define": "^1.3.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "heroku-ssl-redirect": "^0.0.4",
    "image-webpack-loader": "^3.4.2",
    "instantsearch.css": "^7.1.0",
    "js-cookie": "^2.2.0",
    "localforage": "^1.5.2",
    "next": "^7.0.2",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "next-runtime-dotenv": "^1.0.1",
    "node-sass": "^4.9.0",
    "qs": "^6.5.1",
    "raw-loader": "^0.5.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.43",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.1",
    "stripe": "^5.1.1",
    "style-loader": "^0.23.1",
    "typy": "^2.0.1"
  },
  "jest": {
    "verbose": true,
    "bail": false,
    "projects": [
      "jest-stylelint.config.js",
      "jest-eslint.config.js",
      "jest-test.config.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8",
    "babel-jest": "^23.6.0",
    "cypress": "^3.2.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "23.6.0",
    "jest-enzyme": "^7.0.0",
    "jest-puppeteer": "^3.5.0",
    "jest-runner-eslint": "^0.6.0",
    "jest-runner-stylelint": "^1.0.0",
    "nock": "^9.3.3",
    "puppeteer": "^1.10.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-test-utils": "^0.1.3",
    "sass-lint": "^1.12.1",
    "sinon": "^4.0.1",
    "start-server-and-test": "^1.7.12",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.1",
    "supertest": "^3.3.0",
    "sync-request": "^6.0.0",
    "url-loader": "^1.0.1"
  }
}
