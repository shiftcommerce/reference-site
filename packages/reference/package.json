{
  "name": "shift-front-end-react",
  "version": "0.0.1",
  "main": "server.js",
  "repository": "https://github.com/shiftcommerce/shift-front-end-react.git",
  "author": "Shift Commerce",
  "license": "MIT",
  "scripts": {
    "devserver": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "jest-coverage": "jest --coverage",
    "jest-update-snapshot": "jest --updateSnapshot",
    "heroku-postbuild": "next build",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-define": "^1.3.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "next": "^3.0.6",
    "next-redux-wrapper": "^1.3.2",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "localforage": "^1.5.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./spec/support/jestSetup.js"
    ],
    "testRegex": "/spec/(components|objects)/.+\\.spec\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/spec/support/jestFileMock.js"
    }
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "standard": "^10.0.3"
  }
}
