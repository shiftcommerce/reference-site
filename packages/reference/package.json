{
  "name": "shift-front-end-react",
  "version": "0.0.1",
  "main": "server.js",
  "repository": "https://github.com/shiftcommerce/shift-front-end-react.git",
  "author": "Shift Commerce",
  "license": "MIT",
  "scripts": {
    "devserver": "npm rebuild node-sass && NODE_ENV=development node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "jest-coverage": "jest --coverage",
    "jest-update-snapshot": "jest --updateSnapshot",
    "heroku-postbuild": "next build",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.0",
    "next": "^3.0.6",
    "next-redux-wrapper": "^1.3.2",
    "node-sass": "^4.5.3",
    "qs": "^6.5.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-instantsearch": "^4.1.0",
    "react-instantsearch-theme-algolia": "^4.1.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./spec/support/jestSetup.js"
    ],
    "testRegex": "/spec/(components|objects|reducers|pages|lib|actions)/.+\\.spec\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/spec/support/jestFileMock.js"
    }
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-test-utils": "^0.1.2",
    "sinon": "3.2.1",
    "standard": "^10.0.3"
  }
}
