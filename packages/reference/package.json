{
  "name": "shift-front-end-react",
  "version": "0.0.1",
  "main": "server.js",
  "repository": "https://github.com/shiftcommerce/shift-front-end-react.git",
  "author": "Shift Commerce",
  "license": "MIT",
  "engines": {
    "node": "^10.5.0"
  },
  "scripts": {
    "devserver": "npm rebuild node-sass && NODE_ENV=development node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "jest-coverage": "jest --coverage",
    "jest-update-snapshot": "jest --updateSnapshot",
    "heroku-postbuild": "next build",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "dependencies": {
    "@zeit/next-css": "^0.1.5",
    "@zeit/next-sass": "^0.1.2",
    "axios": "^0.16.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "body-parser": "^1.18.2",
    "circular-json": "^0.3.3",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "image-webpack-loader": "^3.4.2",
    "localforage": "^1.5.2",
    "next": "^6.1.0",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.1",
    "raw-loader": "^0.5.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-ideal-image": "^0.0.5",
    "react-instantsearch": "^4.1.3",
    "react-instantsearch-theme-algolia": "^4.1.3",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.7",
    "react-slick": "^0.23.1",
    "react-stickyfill": "^0.2.3",
    "react-stripe-elements": "^1.1.0",
    "react-waypoint": "^8.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.1",
    "slick-carousel": "^1.8.1",
    "stripe": "^5.1.1",
    "style-loader": "^0.19.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "./spec/support/shims.js",
      "./spec/support/enzymeSetup.js",
      "./spec/support/jestSetup.js"
    ],
    "testRegex": "/spec/(components|objects|reducers|pages|lib|actions|requests)/.+\\.spec\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/spec/support/jestFileMock.js"
    }
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "mount",
      "shallow",
      "caches",
      "self",
      "URL",
      "location",
      "fetch",
      "wrapper"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "nock": "^9.3.3",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-test-utils": "^0.1.3",
    "sinon": "^4.0.1",
    "standard": "^10.0.3",
    "url-loader": "^1.0.1"
  }
}
